<UserControl x:Class="Retriever2._0.InfoBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Retriever2._0"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="150" x:Name="InfoBox">
	<Border Style="{StaticResource CustomStyle}">
		<WrapPanel>
			<Label/>
			<TextBlock/>
			<TextBlock/>
		</WrapPanel>
	</Border>
	
</UserControl>
